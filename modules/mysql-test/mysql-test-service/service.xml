<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="mysql.test" auto-namespace-tables="false">
    <namespace>mysqltest</namespace>
    
    <entity remote-service="true" local-service="true" name="Book" table="book" data-source="extDataSource" uuid="false">
        <!-- PK fields -->
        <column name="id" db-name="id" primary="true" type="long"/>
        
        <!-- Other fields -->
        <column name="title" db-name="title" type="String"/>        

    </entity>
</service-builder>
